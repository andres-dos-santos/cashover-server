generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Company {
  id        String   @id @default(uuid())
  name      String
  doc       String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())

  @@map("companies")
}
